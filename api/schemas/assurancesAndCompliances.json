{
  "$id": "assurancesAndCompliances.json",
  "type": "object",
  "required": [],
  "patternProperties": {
    "^.+$": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["title", "checked", "explanation"],
        "properties": {
          "title": {
            "type": "string"
          },
          "checked": {
            "oneOf": [{ "type": "boolean" }, { "type": "null" }]
          },
          "explanation": {
            "type": "string"
          }
        }
      }
    }
  },
  "additionalProperties": false,
  "$schema": "http://json-schema.org/draft-07/schema"
}
