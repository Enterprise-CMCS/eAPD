{
  "name": "@cms-eapd/common",
  "version": "0.0.1",
  "description": "APD app for CMS HITECH program",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "scaffold": "rm -rf dist && mkdir dist",
    "build": "yarn scaffold && babel . --ignore \"node_modules\",\"dist\" -d ./dist -s",
    "stryker": "TZ=utc stryker run",
    "test": "DEBUG_PRINT_LIMIT=10000 TZ=utc jest",
    "test-watch": "DEBUG_PRINT_LIMIT=10000 TZ=utc jest --watch",
    "npm-check": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CMSgov/eAPD.git"
  },
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/CMSgov/eAPD/issues"
  },
  "homepage": "https://github.com/CMSgov/eAPD#readme",
  "engines": {
    "node": "16.16.0"
  },
  "dependencies": {
    "@joi/date": "2.1.0",
    "joi": "17.6.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "6.1.2",
    "@stryker-mutator/jest-runner": "6.1.2"
  },
  "packageManager": "yarn@1.22.18",
  "prettier": "../.prettierrc"
}
